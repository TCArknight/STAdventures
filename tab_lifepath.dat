  <!-- evEvent portal
        Presents a dynamic table where the user can add career events for the
        character.
  -->
  <portal
    id="lpEvents"
    style="tblNormal">
    <table_dynamic
      component="Event"
      showtemplate="evPick"
      choosetemplate="SimpleItem"
	  showfrozenfixed="yes"
      showfixedlast="yes"
      addpick="resEvent"
	  scrollable="no">
      <candidate>!Hide.Event</candidate>
      <titlebar><![CDATA[
        @text = "Add a Event - " & hero.child[resEvent].field[resSummary].text
        ]]></titlebar>
      <description/>
      <headertitle><![CDATA[
        @text = "Events: " & hero.child[resEvent].field[resSummary].text
        ]]></headertitle>
      <additem><![CDATA[
        ~if we're in advancement mode, we've been frozen, so display accordingly
        if (state.iscreate = 0) then
          @text = "{text clrgrey}You cannot add Career Events after initial creation."
          done
          endif

        ~set the color based on whether the proper number of slots are allocated
        if (field[resLeft].value = 0) then
          @text = "{text clrgrey}"
        elseif (field[resLeft].value < 0) then
          @text = "{text clrwarning}"
          endif
        @text &= "Add New Event"
        ]]></additem>
      </table_dynamic>
    </portal>
	
