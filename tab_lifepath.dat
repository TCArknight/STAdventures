      portal[assign].left = portal[lblAssign].right + 5
	  portal[assign].width = width - portal[assign].left -5	  

      height = portal[lblAssign].bottom + 10	  
      ]]></position>

    </template>

 
  <layout
    id="lifepath">
    <portalref portal="hdrFinish" />
    <portalref portal="lpSpecies" 		taborder="10"/>
    <portalref portal="lpEnvironment" 	taborder="20"/>
    <portalref portal="lpUpbringing" 	taborder="30"/>
    <portalref portal="lpAcademy" 		taborder="40"/>
    <portalref portal="lpCareerPath" 	taborder="40"/>
    <portalref portal="lpCareerTrack" 	taborder="40"/>
    <portalref portal="lpCareerExp" 	taborder="50"/>
    <portalref portal="lpEvents" 		taborder="50"/>
    <portalref portal="lpRole" 			taborder="60"/>
    <templateref template="lpService" 	thing="mscService" 	taborder="70"/>
    <templateref template="lpPersonal" 	thing="mscPerson" 	taborder="10"/>

    <!-- This script sizes and positions the layout and its child visual elements. 
	    <portalref portal="lpSpecies" taborder="10"/>
 -->
    <position><![CDATA[
      ~freeze our table in advancement mode to disable adding new choices
      ~Note: All freezing must be done *before* any positioning is performed.
      if (state.iscreate = 0) then
        portal[lpSpecies].freeze = 1
        portal[lpEnvironment].freeze = 1
        portal[lpUpbringing].freeze = 1
        portal[lpAcademy].freeze = 1
        portal[lpCareerPath].freeze = 1
        portal[lpCareerTrack].freeze = 1
        portal[lpCareerExp].freeze = 1
        portal[lpEvents].freeze = 1
        portal[hdrFinish].freeze = 1
	  else		
        portal[lpSpecies].freeze = hero.tagis[LifepathSelected.Species]
        portal[lpEnvironment].freeze = hero.tagis[LifepathSelected.Environment]
        portal[lpUpbringing].freeze = hero.tagis[LifepathSelected.Upbringing]
        portal[lpAcademy].freeze = hero.tagis[LifepathSelected.Academy]
        portal[lpCareerPath].freeze = hero.tagis[LifepathSelected.CareerPath]
        portal[lpCareerTrack].freeze = hero.tagis[LifepathSelected.CareerTrack]
        portal[lpCareerExp].freeze = hero.tagis[LifepathSelected.Experience]
		if (#resleft[resEvent] = 0) then 
          portal[lpEvents].freeze = 1
		  endif
        endif
	  
      ~position and size the table to span the full layout; it will only use the
      ~vertical space that it actually needs
	  var nextBottom as number
	  
	  perform portal[lpSpecies].autoplace[0]
	  perform portal[lpEnvironment].autoplace[10]
	  perform portal[lpUpbringing].autoplace[10]
	  
	  if (hero.tagis[source.2ndEdition] <> 0) then
	    perform portal[lpCareerPath].autoplace[10]
	    perform portal[lpCareerTrack].autoplace[10]
		portal[lpCareerTrack].visible = hero.tagis[Hero.TrackRequired]
		portal[lpAcademy].visible = 0
		portal[lpAcademy].top = height - 60
		nextBottom = portal[lpCareerTrack].bottom
	  else
		perform portal[lpAcademy].autoplace[10]
		portal[lpCareerPath].visible = 0
		nextBottom = portal[lpAcademy].bottom
		endif
		
	  perform portal[lpCareerExp].autoplace[10]
	  perform portal[lpEvents].autoplace[10]
	  nextBottom = portal[lpEvents].bottom

	  portal[hdrFinish].top = nextBottom + 10
      portal[hdrFinish].height = 20
      portal[hdrFinish].width = width
	  nextBottom = portal[hdrFinish].bottom

	  template[lpService].top = nextBottom + 10
      template[lpService].width = (width/2) - 2
	  
	  portal[lpRole].top = nextBottom + 10
      portal[lpRole].left = template[lpService].right + 4
      portal[lpRole].width = (width/2) - 2
	  
	  template[lpPersonal].top  = portal[lpRole].bottom + 10
	  template[lpPersonal].left = portal[lpRole].left
      template[lpPersonal].width = (width/2) - 2
	  
      ]]></position>

    </layout>

  <panel
    id="lifepath"
    name="Lifepath"
    marginhorz="5"
    marginvert="5"
    order="5">
	<live>ShowTab.Lifepath</live>
    <layoutref layout="lifepath"/>
    <position><![CDATA[
      ]]></position>
    </panel>
	
  </document>
