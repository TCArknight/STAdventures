    </thing>
  <thing id="talPrecMan" name="Precision Maneuvering" description="Reduces the Difficulty of the Task by 1, to a minimum of 0, when attempting a Task that requires precise maneuvering, or where there is a collision risk." compset="Talent" uniqueness="useronce">
    <usesource source="1stEdition"/>
    <usesource source="DivCmd"/>
    <tag group="TalentCat" tag="Conn"/>
