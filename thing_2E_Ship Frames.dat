    <bootstrap thing="tal2EAdvSensorSui"></bootstrap>
    <bootstrap thing="tal2EHighResSens"></bootstrap>
    </thing>
  <thing id="mp2EWarpProp" name="Warp Propulsion Pod" description="The pod contains an advanced warp nacelle with paired coils, along with extra fusion reactors that can either assist the ship in maintaining a high warp velocity, or maintain a warp field after saucer separation. \n\nThe ship has +2 Engines and +1 Conn. The ship also gains the Improved Warp Drive and Secondary Reactors talents." compset="MissionPod">
    <fieldval field="bonEngines" value="2"/>
    <fieldval field="bonConn" value="1"/>
    <usesource source="2ndEdition"/>
    <tag group="Ship" tag="MissionPod" name="MissionPod" abbrev="MissionPod"/>
    <tag group="ShipType" tag="typStarship" name="Starship" abbrev="Starship"/>
    <bootstrap thing="tal2EImpWarpDrive"></bootstrap>
    <bootstrap thing="tal2ESecondaryRea"></bootstrap>
    </thing>
  <thing id="mp2EWeapons" name="Weapons" description="The pod contains additional torpedo launchers, phaser arrays, and targeting sensors. \n\nThe ship has +1 Sensors, +1 Weapons, and +1 Security. The ship also has the Fast Targeting Systems and Rapid-Fire Torpedo Launcher talents." compset="MissionPod" summary="The ship has +1 Sensors, +1 Weapons, and +1 Security. The ship also has the Fast Targeting Systems and Rapid-Fire Torpedo Launcher talents.">
    <fieldval field="bonSens" value="1"/>
    <fieldval field="bonWeap" value="1"/>
    <fieldval field="bonSecur" value="1"/>
