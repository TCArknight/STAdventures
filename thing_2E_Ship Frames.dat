    <bootstrap thing="tal2ERapidFireTor"></bootstrap>
    </thing>
  <thing id="ssr2EQuantSlipPul" name="Quantum Slipstream Pulse Drive" description="In the decades since the first Quantum Slipstream Burst Drive was Introduced, several Improvements have been made to the mechanics, however still not to the level of constant operation as the Warp Drive has become. This advancement on the Quantum Slipstream Burst Drive is still experimental, but now allows the ship to enter quantum slipstream for 40 minutes at a time, moving the ship up to 200 light-years before shutting down. The improved drive requires 1 hour to reset instead of the 12 of the previous versions. However, as experimental technology, this drive may require extensive maintenance and be prone to failure. This is an independent system, so the crew may operate the warp drive in between slipstream bursts." compset="StarshipSpecial" summary="Experimental drive, allows for 200LY in 40 minutes before cooldown of 1 hour to reset.">
    <usesource source="2ndEdition"/>
    </thing>
  <thing id="pro2EMultiroleExp" name="Multirole Explorer" description="Many of Starfleet’s most renowned and revered vessels have been jack-of-all-trades ships, rather than specialized for a single type of mission. This versatility allowed the likes of Jonathan Archer, Christopher Pike, James Kirk, and Jean-Luc Picard to explore strange new worlds, seek out new life and new civilizations, and boldly go where no one has gone before.\n\nSYSTEMS: Increase any one system by 1" compset="Profile" uniqueness="unique">
    <fieldval field="proCmd" value="2"/>
    <fieldval field="proConn" value="2"/>
    <fieldval field="proEng" value="2"/>
    <fieldval field="proMed" value="2"/>
    <fieldval field="proSecur" value="2"/>
    <fieldval field="proSci" value="2"/>
    <fieldval field="usrLabel1" value="System:"/>
    <fieldval field="usrCandid1" value="component.AttrSys"/>
    <usesource source="2ndEdition"/>
    <tag group="MisProTal" tag="tal2EImpPowerSys"/>
    <tag group="MisProTal" tag="tal2ERedundantSys"/>
    <tag group="MisProTal" tag="tal2ERuggedDesign"/>
    <tag group="MisProTal" tag="tal2ESecondaryRea"/>
    <tag group="MisProTal" tag="tal2EImpHullInt"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="ShipType" tag="typStarship" name="Starship" abbrev="Starship"/>
    <tag group="User" tag="NoAutoName"/>
    <eval phase="Effects" priority="1000"><![CDATA[if (field[usrChosen1].ischosen <> 0) then
  field[usrChosen1].chosen.field[abiBonus].value += 1
  endif]]></eval>
    </thing>
  <thing id="pro2EEspionageInt" name="Espionage / Intelligence" description="Vessels equipped for espionage are rare in active Starfleet service, but are used by member cultures within the Federation, and by other polities such as the Romulan Empire and Cardassian Union, whose intelligence agencies often employ dedicated vessels (whether or not they are legally permitted to), and in many cultures, intelligence agencies may commandeer existing vessels for their needs. These vessels are often equipped with highly sensitive monitoring devices, enhanced sensors, and stealth systems (though stopping short of actual cloaking devices for Starfleet vessels). These vessels range close to—and sometimes beyond—foreign borders. Serving on these ships is often a dangerous and thankless proposition.\n\nSYSTEMS: Sensors +1" compset="Profile" uniqueness="unique">
    <fieldval field="proCmd" value="2"/>
    <fieldval field="proConn" value="2"/>
    <fieldval field="proEng" value="1"/>
    <fieldval field="proSecur" value="3"/>
    <fieldval field="proMed" value="1"/>
    <fieldval field="proSci" value="3"/>
    <usesource source="2ndEdition"/>
    <tag group="MisProTal" tag="tal2EEWSystems"/>
