<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains the definitions of how the Editor should handle all the
      various publicly accessible objects (i.e. component sets) within the game
      system. A separate "editthing" elemenet is defined for each component set,
      along with the appropriate details for how to configure each facet via
      "inputthing" elements.
-->

<document signature="Hero Lab Data">
  <inputthinggroup
    id="SourceInfo">
	<inputthing
      name="Source Info"
      helptext="Entries related to the source material are below..">
      <it_separator/>
      </inputthing>
    </inputthinggroup>

  <inputthinggroup
    id="WeaponGrp">
    <inputthing
      name="Damage"
      helptext="Damage traits">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Weapon Damage"
      helptext="Specify the damage rating of the weapon.">
      <it_field field="wpDamage"/>
      </inputthing>
    <inputthing
      name="Damage Effects"
      helptext="Select additional damage effects.">
      <it_taglist group="DmgEffect" tag="?"/>
      </inputthing>
    <inputthing
      name="Qualities"
      helptext="Weapon Qualities">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Qualities"
      helptext="Select additional qualities.">
      <it_taglist group="GearQual" tag="?"/>
      </inputthing>
    </inputthinggroup>	 
	
  <inputthinggroup
    id="CostGrp">
    <inputthing
      name="Costs"
      helptext="Weapon Costs">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Cost"
      helptext="Select Esclation/Opportunity costs.">
      <it_taglist group="GearCost" tag="?"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="WeightGrp">
    <inputthing
      name="Weight"
      helptext="Weights">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Weight"
      helptext="Specify the weight of the item.">
      <it_field field="gearWeight"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="DomainGrp">
    <inputthing
      name="User Entry"
      helptext="User Entry">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Disable Auto-Naming?"
      helptext="Specify whether the automatic synthesis of names for this edge should be disabled.">
      <it_tagcheck group="User" tag="NoAutoName"/>
      </inputthing>
    <inputthing
      name="Require Domain Specification?"
      helptext="Specify whether the edge requires the user to specify a domain.">
      <it_tagcheck group="User" tag="NeedDomain"/>
      </inputthing>
    <inputthing
      name="Domain Optional?"
      helptext="Specify whether the specification of a domain is optional.">
      <it_tagcheck group="User" tag="NeedDomainOpt"/>
      </inputthing>
    <inputthing
      name="Domain Term"
      helptext="Specify the term to use as a label for the domain. If empty, the term 'Domain' is used.">
      <it_tagpick group="DomainTerm" tag="?" dynamic="yes"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="ChooseGrp1">
    <inputthing
      name="Selections"
      helptext="User Selections">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Menu #1 Label"
      helptext="Specify the label used to identify choices for display in the first menu. If empty, no first label is shown.">
      <it_field field="usrLabel1"/>
      </inputthing>
    <inputthing
      name="Menu #1 Source"
      helptext="Specify the source from which to retrieve choices for display in the first menu.">
      <it_tagpick group="ChooseSrc1" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #1 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the first menu. If empty, no first menu is shown.">
      <it_field field="usrCandid1"/>
      </inputthing>
    <inputthing
      name="Menu #2 Label"
      helptext="Specify the label used to identify choices for display in the second menu. If empty, no second label is shown.">
      <it_field field="usrLabel2"/>
      </inputthing>
    <inputthing
      name="Menu #2 Source"
      helptext="Specify the source from which to retrieve choices for display in the second menu.">
      <it_tagpick group="ChooseSrc2" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #2 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the second menu. If empty, no second menu is shown.">
      <it_field field="usrCandid2"/>
      </inputthing>
    <inputthing
      name="Menu #3 Label"
      helptext="Specify the label used to identify choices for display in the third menu. If empty, no third label is shown.">
      <it_field field="usrLabel3"/>
      </inputthing>
	  <inputthing
      name="Menu #3 Source"
      helptext="Specify the source from which to retrieve choices for display in the second menu.">
      <it_tagpick group="ChooseSrc3" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #3 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the third menu. If empty, no second menu is shown.">
      <it_field field="usrCandid3"/>
      </inputthing>
	</inputthinggroup>

  <inputthinggroup
    id="ChooseGrp2">
    <inputthing
      name="Selections"
      helptext="User Selections">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Menu #1 Label"
      helptext="Specify the label used to identify choices for display in the first menu. If empty, no first label is shown.">
      <it_field field="usrLabel4"/>
      </inputthing>
    <inputthing
      name="Menu #1 Source"
      helptext="Specify the source from which to retrieve choices for display in the first menu.">
      <it_tagpick group="ChooseSrc4" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #1 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the first menu. If empty, no first menu is shown.">
      <it_field field="usrCandid4"/>
      </inputthing>
    <inputthing
      name="Menu #2 Label"
      helptext="Specify the label used to identify choices for display in the second menu. If empty, no second label is shown.">
      <it_field field="usrLabel5"/>
      </inputthing>
    <inputthing
      name="Menu #2 Source"
      helptext="Specify the source from which to retrieve choices for display in the second menu.">
      <it_tagpick group="ChooseSrc5" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #2 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the second menu. If empty, no second menu is shown.">
      <it_field field="usrCandid5"/>
      </inputthing>
    <inputthing
      name="Menu #3 Label"
      helptext="Specify the label used to identify choices for display in the third menu. If empty, no third label is shown.">
      <it_field field="usrLabel6"/>
      </inputthing>
    <inputthing
      name="Menu #3 Source"
      helptext="Specify the source from which to retrieve choices for display in the second menu.">
      <it_tagpick group="ChooseSrc6" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #3 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the third menu. If empty, no second menu is shown.">
      <it_field field="usrCandid6"/>
      </inputthing>
	</inputthinggroup>

  <!-- Focus editthing -->
  <editthing
    compset="Focus"
    name="Focuses"
    prefix="fo"
    description="Focuses have no special fields - only a name."
    summary="Defines a Focus that can be selected by a character.">
	<inputthing
      name="Categories"
      helptext="select the category the trait belongs in. A starship-specific trait can be selected either in the list, or the checkbox below.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Category"
      helptext="Select the category this Trait belongs to.">
      <it_taglist group="FocusCat" tag="?"/>
      </inputthing>
    <inputthing
      name="Starship Only?"
      helptext="Specify whether this Focus is only for Starships.">
      <it_tagcheck group="FocusCat" tag="Ship"/>
      </inputthing>
    <inputthing>
      <it_groupref id="DomainGrp"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Trait editthing -->
  <editthing
    compset="Trait"
    name="Traits"
    prefix="tr"
    description="Traits have no special fields - only a name. If a trait confers an ability, create it as a special ability and bootstrap it from the trait."
    summary="Defines a Trait that can be selected by a character.">
	<inputthing
      name="Categories"
      helptext="select the category the trait belongs in. A starship-specific trait can be selected either in the list, or the checkbox below.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Category"
      helptext="Select the category this Trait belongs to.">
      <it_tagpick  group="TraitCat" tag="?" require="yes" deftag="Main"/>
      </inputthing>
    <inputthing
      name="Species Trait?"
      helptext="Specify which Species this trait belongs to.">
      <it_tagpick group="SpeciesTrait" tag="?"/>
      </inputthing>
    <inputthing
      name="Starship Only?"
      helptext="Specify whether this Trait is only for Starships.">
      <it_tagcheck group="TraitCat" tag="Ship"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Talent editthing -->
  <editthing
    compset="Talent"
    name="Talents"
    prefix="tal"
    description="Talents usable by heros and starships."
    summary="Defines a Talent.">
    <inputthing
      name="Name for Special Tab"
      helptext="Specify an optional custom name to be used on the Special tab.">
      <it_field field="spcName"/>
      </inputthing>
    <inputthing
      name="Categories"
      helptext="select the category the talent belongs in. A starship-specific talent can be selected either in the list, or the checkbox below.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Category"
      helptext="Select the category this Talent belongs to.">
      <it_taglist group="TalentCat" tag="?"/>
      </inputthing>
    <inputthing
      name="Starship Only?"
      helptext="Specify whether this Talent is only for Starships.">
      <it_tagcheck group="TalentCat" tag="Ship"/>
      </inputthing>
    <inputthing
      name="Other Options"
      helptext="Other options.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Hide from User?"
      helptext="Specify whether this special ability is hidden from direct user selection.">
      <it_tagcheck group="Hide" tag="Talent"/>
      </inputthing>
    <inputthing
      name="Activated by User?"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagcheck group="User" tag="Activation"/>
      </inputthing>
    <inputthing
      name="Creation Only?"
      helptext="Specify whether this special ability can only be selected for a character during creation.">
      <it_tagcheck group="User" tag="CreateOnly"/>
      </inputthing>
    <inputthing>
      <it_groupref id="ChooseGrp1"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>

<!-- These facets of special abilities are not currently used for anything so they are commented out.
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the special ability (default is 0).">
      <it_field field="abiMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the skill(default is 10).">
      <it_field field="abiMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the skill via its abbreviation (max 5 characters).">
      <it_field field="abiAbbrev"/>
      </inputthing>
-->
    </editthing>
	
  <!-- Hero (typMain, typSupp) specific tabe -->
  <!-- Species editthing -->
  <editthing
    group="Hero"
    compset="Species"
    name="Species"
    prefix="spc"
    description="."
