-->

<document signature="Hero Lab Data">

  <!-- Career Events -->
  <thing
    id="evShipDes"
    name="Ship Destroyed"
    compset="Event"
    isunique="yes"
    description="The ship the character was serving on was lost, destroyed during a mission, and the character was one of the few who survived.\n\nAttribute: Daring\nDiscipline: Security">
    <eval index="1" phase="PreAbility" priority="5000">
      <before name="Calc abiFinal"/><![CDATA[
      #abilbonus[attrsys02] += 1
	  #abilbonus[discdept03] += 1
	  
      ]]></eval>    
    </thing>

