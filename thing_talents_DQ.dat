  <thing id="talFavoredFortune" name="Favored By Fortune" description="If the character is successful on a Task, the next Task performed during that scene gains one bonus Momentum if successful." compset="Talent" uniqueness="useronce">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="TalentCat" tag="Racial"/>
    <pickreq thing="spcAnkari"/>
    </thing>
  <thing id="talVibrationSense" name="Vibration Senses" description="The character has learned to use their unique physiology to allow them to detect vibrations, enhancing their ability to see and hear, even in darkness. Characters with this Talent reduce the Difficulty of perception-related Tasks by 1" compset="Talent" summary="Reduce the Difficulty of perception-related Tasks by 1." uniqueness="useronce">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="TalentCat" tag="Racial"/>
    <pickreq thing="spcAnkari"/>
    </thing>
  <thing id="talMaxEfficiency" name="Maximized Efficiency" description="When assisting another character, the Jye may re-roll their assist die. In addition, if the assist die generated one or more successes, the lead character gains a bonus Momentum if the Task is successful." compset="Talent" summary="When assisting, may re-roll assist die. Also, if 1 or more successes, lead gains a bonus Momentum if task successful." uniqueness="useronce">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="TalentCat" tag="Racial"/>
    <pickreq thing="spcJye"/>
    </thing>
  <thing id="talNatCoordinator" name="Natural Coordinator" description="When assisting, or being assisted, the character is always considered to have an applicable Focus for that Task. In addition, the character counts as having a Command of 4 for the purposes of learning Talents." compset="Talent" summary="When assisting, may re-roll assist die. Also, if 1 or more successes, lead gains a bonus Momentum if task successful." uniqueness="useronce">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="TalentCat" tag="Racial"/>
    <pickreq thing="spcJye"/>
    </thing>
  <thing id="talBorgImplants" name="Borg Implants" description="A Liberated Borg character must select this Talent during character creation. When this Talent is chosen, pick one, two, or three Borg Implants (see sidebar). The character gains the benefits of those implants, but the Difficulty of Medicine Tasks performed on them increases by the number of implants they have. The character also increases the Complication range of all social interaction Tasks by the number of implants they have. An ordinary milestone may be used to remove one implant at a time, not the whole Talent; once all implants have been removed, this Talent can be\nswapped for another." compset="Talent" summary="Difficulty of Medicine Tasks  on character and the Complication Range of all social interactions is increased by number of implants" uniqueness="useronce">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="User" tag="CreateOnly"/>
    <tag group="TalentCat" tag="Racial"/>
    <exprreq message="Liberated Borg required."><![CDATA[#isspecies[spcLiberatedBorg] <> 0]]></exprreq>
    </thing>
  <thing id="talDirNeuralInter" name="Direct Neural Interface" description="Having existed as an extension of one of the largest and most complex computer networks in the known Galaxy, Liberated Borg possess a natural affinity for computers and data processing equipment of all kinds, and are almost organic computers themselves. When the character attempts a Task that involves, or is assisted by, a computer (including the ship’s Computers system), the character gains a bonus d20 for their dice pool." compset="Talent" summary="When the character attempts a Task that involves, or is assisted by, a computer (including the ship’s Computers system), the character gains a bonus d20 for their dice pool." uniqueness="useronce">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="TalentCat" tag="Racial"/>
    <exprreq message="Liberated Borg required."><![CDATA[#isspecies[spcLiberatedBorg] <> 0]]></exprreq>
    </thing>
  <thing id="eqBIAdaptShield" name="Adaptive Shielding" description="Each time the character is Injured by an energy-based weapon, roll 1 Damage Die. If an Effect is rolled, the character becomes immune to that type of weapon for the remainder of the scene." compset="Equipment">
    <usesource source="QuadDelta"/>
    <usesource source="1stEdition"/>
    <tag group="GearCat" tag="Cyber"/>
    <tag group="Helper" tag="BorgImplant" name="Borg Implant"/>
    <exprreq message="Requires Borg Implants talent."><![CDATA[#hastalent[talBorgImplants] <> 0]]></exprreq>
    </thing>
