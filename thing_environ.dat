    <fieldval field="usrCandid1" value="component.AttrSys &amp; (thingid.attrsys06|thingid.attrsys04)"/>
    <fieldval field="usrSource1" value="1"/>
    <fieldval field="usrLabel2" value="+1 to Discipline:"/>
    <fieldval field="usrCandid2" value="component.DiscDept &amp; (thingid.discdept04|thingid.discdept06|thingid.discdept05)"/>
