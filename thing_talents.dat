  <thing id="talCollab" name="Collaboration" description="You must choose a single Discipline when you select\nthis Talent. Whenever an ally attempts a Task using that\nDiscipline, you may spend one Momentum (Immediate) to\nallow them to use your score for that Discipline, and one of\nyour Focuses." compset="Talent" uniqueness="useronce">
    <fieldval field="usrCandid1" value="component.DiscDept"/>
